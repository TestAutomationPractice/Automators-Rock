<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Conferencing Smoke Suite" parallel="classes"
	thread-count="1" configfailurepolicy="continue">
	<parameter name="platform" value="Windows 7" />
	<parameter name="browser" value="Chrome" />
	<parameter name="version" value="61.0" />
	<parameter name="screenResolution" value="1280x960" />
	<listeners>
		<!-- <listener class-name="Miscellaneous.Listeners.MultiBrowserSuiteListener" 		 -->	
		<listener class-name="Miscellaneous.ConfigureSuiteListener" />
		<!-- <listener class-name="Miscellaneous.Listeners.TestCaseListener" /> 
			<listener class-name="Miscellaneous.Listeners.AnnotationTransformer" /> -->
	</listeners>
	<test name="Smoke Suite Live Stream" preserve-order="true">
		<classes>
			<class name="Conferencing.test.SmokeTestLiveStream" />
		</classes>
	</test>
	<test name="Smoke Suite Conf Call Without Live Stream" preserve-order="true">
		<classes>
			<class name="Conferencing.test.SmokeTestLiveStreamDisabled" />
		</classes>
	</test>
</suite>